{
    "name": "wegmeister/hyphenator",
    "type": "typo3-flow-plugin",
    "description": "PHP-Adaption of Hyphenator.js (by Mathias Nater).",
    "license": "Creative Commons Attribution-Share Alike 2.5 Switzerland",
    "require": {
        "php": ">=7.0",
        "org_heigl/hyphenator": "2.0.*",
        "typo3/neos": "*"
    },
    "autoload": {
        "psr-4": {
            "Wegmeister\\Hyphenator\\": "Classes"
        }
    },
    "extra": {
        "typo3/flow": {
            "manage-resources" : true
        },
        "neos": {
            "installer-resource-folders": ["Resources/Private/Installer/"]
        }
    }
}
